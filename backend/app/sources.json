  "web_sources": [
    {
      "name": "AIEra (新智元)",
      "url": "https://aiera.com.cn",
      "category": "news",
      "enabled": true,
      "language": "zh",
      "priority": 1,
      "source_type": "web",
      "description": "国内影响力最广泛的人工智能社交资讯平台",
      "extra_config": {
        "max_articles": 20,
        "article_selector": "article",
        "title_selector": "h2 a",
        "link_selector": "h2 a",
        "date_selector": "time",
        "fetch_full_content": true,
        "content_selector": "article .entry-content, article > div",
        "author_selector": "ul li a[href*='/author/']"
      }
    },
    {
      "name": "Anthropic Research",
