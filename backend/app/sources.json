      "extra_config": {
        "max_articles": 20,
        "article_selector": "a.PublicationList-module-scss-module__KxYrHG__listItem",
        "title_selector": ".PublicationList-module-scss-module__KxYrHG__title",
        "link_selector": "self",
        "date_selector": ".PublicationList-module-scss-module__KxYrHG__date",
        "description_selector": ".PublicationList-module-scss-module__KxYrHG__subject"
